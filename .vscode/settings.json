{
    "turboConsoleLog.addSemicolonInTheEnd": true,
    "turboConsoleLog.logMessagePrefix": "RD",
    "turboConsoleLog.quote": "`",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
